{
  "compilerOptions": {
    "target": "es6",
    "outDir": "dist",
    "rootDir": "src",
    "module": "commonjs",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    // Retirar
    "skipLibCheck": true
  },
  "include": ["src"],
  "exclude": ["node_modules"]
}
